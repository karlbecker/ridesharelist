{
  "globals": {
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "quotes": [2, "single", {"avoidEscape": true, "allowTemplateLiterals": true}],
    "eqeqeq": 0,
    "no-path-concat": 1,
    "space-infix-ops": 0,
    "key-spacing": 0,
    "no-process-exit": 0,
    "camelcase": 0,
    "curly": 0,
    "no-mixed-requires": 0,
    "comma-dangle": 0,
    "no-shadow": 0,
    "comma-spacing": 0,
    "consistent-return": 0,
    "handle-callback-err": 2,
    "strict": 0,
    "yoda": 0,
    "space-unary-ops": 0,
    "no-underscore-dangle": 0,
    "new-cap":0,
    "no-unused-vars": [2, {"args": "none"}],
    "semi": 2,
    "no-multi-spaces": 2,
    "no-use-before-define": [2, {"functions": false, "classes": false}],
    "no-undef": 2,
    "semi-spacing": 2,
    "no-spaced-func": 2,
    "no-regex-spaces": 2,
    "no-empty": 2,
    "no-redeclare": 2,
    "no-cond-assign": 2,
    "no-empty-character-class": 2

  }

}
